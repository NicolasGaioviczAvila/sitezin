{
    "version": 2,
    "builds": [
        {
            "src": "./api/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "build": {
        "env": {
            "NODE_ENV": "production"
        },
        "installCommand": "apt-get update && apt-get install -y unixodbc-dev && npm install"
    }
}